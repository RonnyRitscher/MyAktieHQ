<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <!-- User application theme. -->
    <style name="AppTheme_User" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorUserPrimary</item>
        <item name="colorPrimaryDark">@color/colorUserPrimaryDark</item>
        <item name="colorAccent">@color/colorUserAccent</item>
    </style>

</resources>
